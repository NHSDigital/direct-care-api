{
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "direct-care-api:SdsFunction",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/template.yaml",
                "logicalId": "SdsFunction"
            },
            "lambda": {
                "payload": {
                    "json": {
                        "ods": "Y12345",
                    }
                },
                "environmentVariables": {}
            }
        },
